@article{ryalatRobustIDAPBCApproach2018,
  ids = {ryalat2018},
  title = {A {{Robust IDA}}-{{PBC Approach}} for {{Handling Uncertainties}} in {{Underactuated Mechanical Systems}}},
  author = {Ryalat, Mutaz and Laila, Dina Shona},
  year = {2018},
