  title = {Data-Driven Design of Two Degree-of-Freedom Nonlinear Controllers: {{The D2}}-{{IBC}} Approach},
  author = {Novara, Carlo and Formentin, Simone and Savaresi, Sergio M. and Milanese, Mario},
