@inproceedings{selvi2018,
  title = {Towards Direct Data-Driven Model-Free Design of Optimal Controllers},
  booktitle = {2018 {{European Control Conference}} ({{ECC}})},
  author = {Selvi, Daniela and Piga, Dario and Bemporad, Alberto},
  year = {2018},
  month = jun,
