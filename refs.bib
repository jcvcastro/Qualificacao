@article{tanaskovic2017,
  title = {Data-Driven Control of Nonlinear Systems: {{An}} on-Line Direct Approach},
  author = {Tanaskovic, Marko and Fagiano, Lorenzo and Novara, Carlo and Morari, Manfred},
  year = {2017},
