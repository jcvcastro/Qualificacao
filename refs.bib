@article{adiguzelDiscretetimeBacksteppingControl2019,
  title = {Discrete-Time Backstepping Control with Nonlinear Adaptive Disturbance Attenuation for the Inverted-Pendulum System},
  author = {Ad{\i}g{\"u}zel, Fatih and Yal{\c c}{\i}n, Yaprak},
  year = {2019},
  month = aug,
